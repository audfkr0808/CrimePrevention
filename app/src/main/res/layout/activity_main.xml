<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jgh.myapplication.Activity.MainActivity">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">
        <TextView
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:text="전국지도"
            android:textSize="30dp"
            android:textColor="#F6F6F6"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="245dp"/>
        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/allmap"
             />

        <Button
            android:id="@+id/btn_Seoul"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="130dp"
            android:background="#00ff0000"
            android:onClick="onClickSeoul"
            android:text="서울"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Incheon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="105dp"
            android:background="#00ff0000"
            android:onClick="onClickIncheon"
            android:text="인천"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Gyeunggi"
            android:layout_width="45dp"
            android:layout_height="30dp"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="160dp"
            android:background="#00ff0000"
            android:onClick="onClickGyeonggido"
            android:text="경기도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Gangwon"
            android:layout_width="45dp"
            android:layout_height="30dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="100dp"
            android:background="#00ff0000"
            android:onClick="onClickGangwondo"
            android:text="강원도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Chungnam"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="240dp"
            android:background="#00ff0000"
            android:onClick="onClickChungnam"
            android:text="충청남도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Chungbuk"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_marginLeft="165dp"
            android:layout_marginTop="200dp"
            android:background="#00ff0000"
            android:onClick="onClickchungbuk"
            android:text="충청북도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Gyeongbuk"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_marginLeft="230dp"
            android:layout_marginTop="250dp"
            android:background="#00ff0000"
            android:onClick="onClickGyeongbuk"
            android:text="경상북도"
            android:textColor="#F6F6F6"
            android:textStyle="bold"
/>

        <Button
            android:id="@+id/btn_DaeJeon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="270dp"
            android:background="#00ff0000"
            android:onClick="onClickDaejeon"
            android:text="대전"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Daegu"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="230dp"
            android:layout_marginTop="325dp"
            android:background="#00ff0000"
            android:onClick="onClickDaegu"
            android:text="대구"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Jeonbuk"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="330dp"
            android:background="#00ff0000"
            android:onClick="onClickjeonbuk"
            android:text="전라북도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Gwangju"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="95dp"
            android:layout_marginTop="400dp"
            android:background="#00ff0000"
            android:onClick="onClickGwangju"
            android:text="광주"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Jeonnam"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_alignLeft="@+id/btn_Chungnam"
            android:layout_alignStart="@+id/btn_Chungnam"
            android:layout_below="@+id/btn_Gwangju"
            android:background="#00ff0000"
            android:onClick="onClickJeonnam"
            android:text="전라남도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Gyeongnam"
            android:layout_width="55dp"
            android:layout_height="30dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="380dp"
            android:background="#00ff0000"
            android:onClick="onClickGyeongnam"
            android:text="경상남도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Ulsan"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="290dp"
            android:layout_marginTop="355dp"
            android:background="#00ff0000"
            android:onClick="onClcikUlsan"
            android:text="울산"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Busan"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="395dp"
            android:background="#00ff0000"
            android:onClick="onClickBusan"
            android:text="부산"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Jeju"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="535dp"
            android:background="#00ff0000"
            android:onClick="onClickJejudo"
            android:text="제주도"
            android:textColor="#F6F6F6"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_result"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="550dp"
            android:background="@drawable/plus"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_ccc"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginLeft="265dp"
            android:layout_marginTop="490dp"
            android:text="비교"
            android:drawableLeft="@drawable/com"
            android:visibility="gone"
            android:onClick="onClickResult2"
            android:textStyle="bold"
            />
        <Button
            android:id="@+id/btn_ddd"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginLeft="265dp"
            android:layout_marginTop="450dp"
            android:drawableLeft="@drawable/ran"
            android:text="순위"
            android:onClick="onClickResult3"
            android:visibility="gone"
            android:textStyle="bold" />
        <Button
            android:id="@+id/btn_aaa"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginLeft="265dp"
            android:layout_marginTop="410dp"
            android:text="분석"
            android:onClick="onClickResult1"
            android:drawableLeft="@drawable/gg"
            android:visibility="gone"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="200dp"
            android:layout_height="80dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/col" />
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
